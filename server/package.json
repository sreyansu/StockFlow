{
  "name": "stockflow-pro-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config src/server.ts",
    "db:migrate": "ts-node -r dotenv/config src/scripts/migrate.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.7",
    "@types/pg": "^8.11.6",
    "concurrently": "^9.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
